{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/vue-starter-kit",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "google/protobuf": "*",
        "grpc/grpc": "*",
        "hanafalah/klinik-starterpack": "1.x-dev as 1.0",
        "hanafalah/microtenant": "1.x-dev as 1.0",
        "inertiajs/inertia-laravel": "^2.0",
        "knuckleswtf/scribe": "*",
        "kreait/laravel-firebase": "^6.0",
        "laravel/framework": "^12.0",
        "laravel/octane": "^2.11",
        "laravel/prompts": "^0.3.5",
        "laravel/sanctum": "^4.0",
        "laravel/telescope": "^5.9",
        "laravel/tinker": "^2.10.1",
        "nunomaduro/pokio": "^0.1.0",
        "opcodesio/log-viewer": "^3.17",
        "predis/predis": "^3.0",
        "tightenco/ziggy": "^2.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "*",
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.18",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2"
    },
    "autoload": {
        "files": [
            "app/helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "dev:ssr": [
            "npm run build:ssr",
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"php artisan inertia:start-ssr\" --names=server,queue,logs,ssr"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "hanafalah/klinik-starterpack": {
            "type": "path",
            "url": "repositories/klinik-starterpack",
            "optimize": {
                "symlink": true
            }
        },
        "hanafalah/laravel-support": {
            "type": "path",
            "url": "repositories/laravel-support",
            "optimize": {
                "symlink": true
            }
        },
        "hanafalah/module-service": {
            "type": "path",
            "url": "repositories/module-service",
            "optimize": {
                "symlink": true
            }
        },
        "hanafalah/microtenant": {
            "type": "path",
            "url": "repositories/microtenant",
            "optimize": {
                "symlink": true
            }
        },
        "hanafalah/api-helper": {
            "type": "path",
            "url": "repositories/api-helper",
            "options": {
                "symlink": true
            }
        },
        "hanafalah/laravel-feature": {
            "type": "path",
            "url": "repositories/laravel-feature",
            "options": {
                "symlink": true
            }
        },
        "hanafalah/module-user": {
            "type": "path",
            "url": "repositories/module-user",
            "options": {
                "symlink": true
            }
        },
        "hanafalah/module-regional": {
            "type": "path",
            "url": "repositories/module-regional",
            "options": {
                "symlink": true
            }
        },
        "hanafalah/module-transaction": {
            "type": "path",
            "url": "repositories/module-transaction",
            "options": {
                "symlink": true
            }
        },
        "hanafalah/module-workspace": {
            "type": "path",
            "url": "repositories/module-workspace",
            "options": {
                "symlink": true
            }
        },
        "hanafalah/laravel-permission": {
            "type": "path",
            "url": "repositories/laravel-permission",
            "options": {
                "symlink": true
            }
        },
        "hanafalah/laravel-package-generator": {
            "type": "path",
            "url": "repositories/laravel-package-generator",
            "options": {
                "symlink": true
            }
        },
        "hanafalah/module-encoding": {
            "type": "path",
            "url": "repositories/module-encoding",
            "options": {
                "symlink": true
            }
        }
    }
}